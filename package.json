{
  "name": "cdk-template-api",
  "version": "1.0.0",
  "description": "AWS CDK Template with NestJS",
  "main": "index.js",
  "scripts": {
    "build": "nest build",
    "build:lambda": "tsc -p tsconfig.lambda.json",
    "build:all": "pnpm build && pnpm build:lambda",
    "prebuild": "rimraf dist",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "cdk": "cdk",
    "cdk:deploy": "pnpm build:all && cdk deploy",
    "cdk:destroy": "cdk destroy",
    "cdk:synth": "pnpm build:all && cdk synth",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:diff": "cdk diff",
    "cdk:ls": "cdk ls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "@aws-sdk/client-sts": "^3.0.0",
    "@codegenie/serverless-express": "^4.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.7",
    "joi": "^17.13.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.0",
    "@types/express": "^4.17.0",
    "@types/joi": "^17.2.3",
    "@types/node": "^20.0.0",
    "aws-cdk": "^2.0.0",
    "rimraf": "^5.0.10",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
